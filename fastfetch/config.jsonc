      "key": "├ Local IP",
      "keyColor": "175",
      "format": "\u001b[38;5;109m{1}\u001b[0m"
    },
    {
      "type": "battery",
      "key": "├ Battery",
      "keyColor": "214",
      "format": "\u001b[38;5;65m{4}\u001b[0m \u001b[38;5;175m[{5}]\u001b[0m"
    },
    {
      "type": "poweradapter",
      "key": "├ Power",
      "keyColor": "167",
      "format": "\u001b[38;5;214m{1}\u001b[0m"
