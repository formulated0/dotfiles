      "keyColor": "142",
      "format": "\u001b[38;5;109m{1}\u001b[0m"
    },
    {
      "type": "publicip", 
      "key": "├ Public IP",
      "keyColor": "109",
      "format": "\u001b[38;5;142m{1}\u001b[0m"
    },
    {
      "type": "datetime",
      "key": "├ Time",
      "keyColor": "175",
      "format": "\u001b[38;5;208m{8}, {5} {11}, {14}:{18}\u001b[0m"
    },
    {
      "type": "separator",
      "string": "─"
    },
    {
      "type": "colors",
      "key": "╰─ Colors",
      "symbol": "circle"
    }
  ]
