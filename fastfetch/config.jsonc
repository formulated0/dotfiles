{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
  "source": "/home/formulated/.config/fastfetch/uhm-colorized_fastfetch.png",
  "height": 17,
  "width": 40,
  "padding": {
    "top": 2,
    "left": 2
  }
},
  "display": {
    "separator": " ",
    "size": {
      "binaryPrefix": "iec"
    },
    "color": {
      "output": "223"
    }
  },
  "modules": [
    {
      "type": "title",
      "key": "╭─",
      "keyColor": "214"
    },
    {
      "type": "separator",
      "string": "─"
    },
    {
      "type": "os",
      "key": "├ OS",
      "keyColor": "208",
      "format": "\u001b[38;5;65m{2} {12}\u001b[0m"
    },
    {
      "type": "kernel", 
      "key": "├ Kernel",
      "keyColor": "214",
      "format": "\u001b[38;5;109m{1} {2}\u001b[0m"
    },
    {
      "type": "uptime",
      "key": "├ Uptime", 
      "keyColor": "142",
      "format": "\u001b[38;5;175m{1}d {2}h {3}m\u001b[0m"
    },
    {
      "type": "packages",
      "key": "├ Packages",
      "keyColor": "109",
      "format": "\u001b[38;5;214m{1}\u001b[0m \u001b[38;5;142m({2})\u001b[0m"
    }, // dadad
    {
      "type": "shell",
      "key": "├ Shell",
      "keyColor": "175",
      "format": "\u001b[38;5;109m{1} {2}\u001b[0m"
    },
    {
      "type": "display",
      "key": "├ Display",
      "keyColor": "208",
      "format": "\u001b[38;5;65m{1}×{2}\u001b[0m @ \u001b[38;5;214m{3} Hz\u001b[0m"
    },
    {
      "type": "de",
      "key": "├ DE",
      "keyColor": "142",
      "format": "\u001b[38;5;175m{1}\u001b[0m"
    },
    {
      "type": "wm",
      "key": "├ WM",
      "keyColor": "109",
      "format": "\u001b[38;5;214m{1}\u001b[0m \u001b[38;5;109m({3})\u001b[0m"
    },
    {
      "type": "terminal",
      "key": "├ Terminal",
      "keyColor": "175",
      "format": "\u001b[38;5;142m{1}\u001b[0m"
    },
    {
      "type": "theme",
      "key": "├ GTK Theme",
      "keyColor": "214",
