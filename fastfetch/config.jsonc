    },
    {
      "type": "font",
      "key": "├ Font",
      "keyColor": "208",
      "format": "\u001b[38;5;65m{1}\u001b[0m"
    },
    {
      "type": "separator",
      "string": "─"
    },
    {
      "type": "cpu",
      "key": "├ CPU",
      "keyColor": "167",
      "format": "\u001b[38;5;214m{1}\u001b[0m \u001b[38;5;196m({5}) @ {7}\u001b[0m"
    },
    {
      "type": "gpu", 
      "key": "├ GPU",
      "keyColor": "142",
      "format": "\u001b[38;5;214m{2}\u001b[0m \u001b[38;5;109m[{8}/{7}]\u001b[0m @ \u001b[38;5;175m{4}\u001b[0m"
    },
    {
      "type": "memory",
      "key": "├ Memory", 
      "keyColor": "208",
      "format": "\u001b[38;5;196m{1}\u001b[0m / \u001b[38;5;65m{2}\u001b[0m \u001b[38;5;214m({3})\u001b[0m"
    },
    {
      "type": "disk",
